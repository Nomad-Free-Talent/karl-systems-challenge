  "query": "\n            SELECT p.id, p.name, p.resource, p.action, p.created_at\n            FROM permissions p\n            INNER JOIN role_permissions rp ON p.id = rp.permission_id\n            WHERE rp.role_id = $1\n            ORDER BY p.name\n            ",
